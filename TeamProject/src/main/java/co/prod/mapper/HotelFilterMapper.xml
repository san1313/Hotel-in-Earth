<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.prod.mapper.HotelFilterMapper">
  <select id="getHotelList" resultType="HotelsVO">
  select * from hotels
  </select>
  
  <select id="getHotelInfo" resultType="HotelsVO">
  select * from hotels where hotel_id = #{id}
  </select>
</mapper>